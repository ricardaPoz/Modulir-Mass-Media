<Application x:Class="Modulir_Mass_Media.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Modulir_Mass_Media"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="SansBold">
            <Setter Property="TextElement.FontFamily" Value="/icons/#Yahfie Bruch"/>
        </Style>
        <Style x:Key="SansRegular">
            <Setter Property="TextElement.FontFamily" Value="\icons\ptsans-regular.ttf"/>
        </Style>
    </Application.Resources>
</Application>
